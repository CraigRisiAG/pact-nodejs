{
  "consumer": {
    "name": "Frontend"
  },
  "provider": {
    "name": "ClientsService"
  },
  "interactions": [
    {
      "description": "a request for all clients",
      "providerState": "i have a list of clients",
      "request": {
        "method": "GET",
        "path": "/clients",
        "headers": {
          "Accept": "application/json, text/plain, */*"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": [      
        {
          "firstName": "Anakin",
          "lastName": "Skywalker",
          "age": 16,
          "id": 1
        },
        {
          "firstName": "Luke",
          "lastName": "Skywalker",
          "age": 25,
          "id": 2
        },
        {
          "firstName": "Obiwan",
          "lastName": "Kenobi",
          "age": 49,
          "id": 3
        }
        ]
      }
    },
    {
      "description": "a request to create client with firstname and lastname",
      "providerState": "i create a new client",
      "request": {
        "method": "POST",
        "path": "/clients",
        "headers": {
          "Content-Type": "application/json;charset=utf-8"
        },
        "body": {
          "firstName": "Rafaela",
          "lastName": "Azevedo",
          "age": 29
        }
      },
      "response": {
        "status": 200,
        "headers": {
        },
        "body": {
          "firstName": "Rafaela",
          "lastName": "Azevedo",
          "age": 29,
          "id": 3
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}